<body>


<div id="productos-form-container">
    <h1>Ver lista de productos</h1>
    <div id="productos-list"></div>
    <form id="agregar-producto-form">
        <label for="nombre">Nombre del producto:</label>
        <input type="text" id="nombre" required>
        <br>

        <label for="precio">Precio del producto:</label>
        <input type="text" id="precio" required>
        <br>

        <label for="cantidad">Cantidad:</label>
        <input type="text" id="cantidad" required>
        <br>

        <button type="submit">Agregar Producto</button>
   
        <button id="eliminar-ultimo" type="button">Eliminar producto</button>


    </form>
</div>

    <script src="/socket.io/socket.io.js"></script>
    <script src="/scripts.js"></script>

